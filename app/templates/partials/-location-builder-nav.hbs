        <li class="builder__list--nested">
          <ul>
            <li class="builder__nav__item">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'visualization' entity_and_id 'industries' 'treemap' (query-params variable='employment') class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.industries.employment' name=name}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'visualization' entity_and_id 'industries' 'treemap' (query-params variable='wages') class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.industries.wages' name=name}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item builder__nav__item--unavailable">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'graph_builder' (query-params entity=entity entity_id=entity_id data_source="inudstrie") class="builder__nav__question"}}
