                {{/link-to}}
              </div>
            </li>
          </ul>
        </li>
      </ul>
    {{/ivy-tab-panel}}

    {{!-- IMPORT AND EXPORT --}}
    {{#ivy-tab-panel classNames='tabmod__card' activeClass='tabmod__card--is--active'}}
      <ul class="builder__nav__list">
        <li class="builder__list--nested">
          <ul>
            <li class="builder__nav__item">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'visualization' entity_and_id 'products' 'treemap' (query-params variable='export_value') class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.export_value' name=name}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item builder__nav__item--unavailable">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'graph_builder' (query-params entity=entity entity_id=entity_id data_source="products") class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.export_value_to' name=name place=builderNavDropDown}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'visualization' entity_and_id 'products' 'treemap' (query-params variable='import_value') class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.import_value' name=name}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item builder__nav__item--unavailable">
              <i class="icon-cidcon_treemap builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Treemap</h4>
                {{#link-to 'graph_builder' (query-params entity=entity entity_id=entity_id data_source="products") class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.import_value_from' name=name place=builderNavDropDown}}
                {{/link-to}}
              </div>
            </li>
          </ul>
        </li>
        <li class="builder__list--nested">
          <ul>
            <li class="builder__nav__item">
              <i class="icon-cidcon_scatter builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Scatterplot</h4>
                {{#link-to 'visualization' entity_and_id 'products' 'scatter' (query-params variable='null' endDate=2014 startDate=2013) class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.scatter' name=name}}
                {{/link-to}}
              </div>
            </li>
            <li class="builder__nav__item">
              <i class="icon-cidcon_similarity builder__nav__item__icon"></i>
              <div class="builder__nav__item__textwrap">
                <h4 class="label builder__nav__label">Similarity map</h4>
                {{#link-to 'visualization' entity_and_id 'products' 'similarity' (query-params variable='null' endDate=2014 startDate=2013) class="builder__nav__question"}}
                  {{t 'graph_builder.builder_nav.location.products.similarity' name=name}}
                {{/link-to}}
              </div>
            </li>
          </ul>
        </li>
      </ul>
    {{/ivy-tab-panel}}

    {{!-- INDUSTRIES --}}
