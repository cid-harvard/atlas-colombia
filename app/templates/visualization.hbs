{{component "stream-header" model=model.entity type=entityType }}
<header class="section__header builder__header">
  <h3 class="section__head">{{pageTitle}}</h3>
</header>
<section class="stream__section">
  <div {{bind-attr class=":section__buildermod drawerSettingsIsOpen:buildermod__settings--open drawerChangeGraphIsOpen:buildermod__changegraph--open" }}>
    <header class="buildermod__header">
      <h4 class="buildermod__head">{{builderModHeader}}: {{headerValue}} ({{years}})</h4>
      <div class="buildermod__tools">
          <button class="btn btn--standalone btn--buildermod buildermod__tool buildermod__tool--settings" {{action 'toggleDrawerSettings'}} data-settings-open=''>
            {{fa-icon icon= "plus" classNames="tool__btn__icon"}}
            {{t 'graph_builder.settings.label'}}
          </button>
          <button class="btn btn--standalone btn--buildermod buildermod__tool" data-change-question='' {{action 'toggleDrawerChangeGraph'}}>
            {{fa-icon icon= "plus" classNames="tool__btn__icon"}}
            {{t 'graph_builder.change_graph.label'}}
          </button>
      </div>
    </header>
    <div {{bind-attr class=":buildermod__vizwrap isFixedHeight:buildermod__vizwrap--fixedheight"}}>
      {{#unless isGeo }}
        {{partial "partials/buildermod-search"}}
      {{/unless}}
      {{component 'graphbuilder-settings'
        startDate=startDate
        endDate=endDate
        dateExtent=dateExtent
        dateRange=dateRange
        isOpen=drawerSettingsIsOpen
        isSingleYear=isSingleYear
        isScatter=isScatter
        rcaFilter=rcaFilter
        years=years
      }}
      {{partial "partials/buildermod-drawer-change-graph"}}
      {{component visualizationComponent
        classNames='buildermod__viz'
        immutableData=immutableData
        data=filteredData
        dataType=source
        varDependent=varDependent
        startDate=startDate
        dateExtent=dateExtent
        endDate=endDate
        metadata=metadata
        isFiltered=isFiltered
        entityType=entityType
        entityId=entityId
        variable=variable
        search=search
        }}
    </div>
    {{component 'visualization-explanation'
      text=visualizationExplanation
    }}
    <div class="ember__table__wrap">
      {{component 'table-download'
        name=builderModHeader
        data=immutableData
        source=source
        pageTitle=pageTitle
      }}
      {{component 'graphbuilder-table'
        data=filteredData
        source=source
        isSingleYear=isSingleYear
        search=search
      }}
    </div>
    <small class="table__footer">
      {{ t 'graph_builder.table.read_more' }}
      {{#link-to 'about.glossary' class='link--stream link--stream--inline--tiny table__footer__link--glossary'}}
        {{t 'general.glossary'}}.
      {{/link-to}}
    </small>
  </div>
</section>
<section class="stream__section recirc builder__recirc">
  <div class="recirc__bground" style="background-image: url({{recircUrl}})"></div>
  <div class="recirc__wrap">
    <h3 class="section__head recirc__head">
      {{recircCopy}}
    </h3>
  </div>
</section>

